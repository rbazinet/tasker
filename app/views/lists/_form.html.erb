<div class="card mb-3" id="list-<%= list.client_id %>">
  <div class="card-header">
    New List
  </div>
  <div class="card-body">
    <%= bootstrap_form_with model: list, local: false do |form| %>
      <%= form.hidden_field :client_id %>
      <%= form.text_field :name %>
      <%= form.primary %>
    <% end %>  
  </div>
</div>
